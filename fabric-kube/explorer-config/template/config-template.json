{
	"name": "first-network",
	"version": "1.0.0",
	"license": "Apache-2.0",
	"client": {
		"tlsEnable": false,
		"adminCredential": {
			"id": "admin",
			"password": "adminpw"
		},
		"enableAuthentication": false,
		"organization": "${ORG_NAME}MSP",
		"connection": {
			"timeout": {
				"peer": {
					"endorser": "300"
				},
				"orderer": "300"
			}
		}
	},

	"channels": {
		"mychannel": {
			"peers": {
				"hlf-peer--${ORG_NAME}--peer0": {}
			},
			"connection": {
				"timeout": {
					"peer": {
						"endorser": "6000",
						"eventHub": "6000",
						"eventReg": "6000"
					}
				}
			}
		}
	},
	"organizations": {
		"${ORG_NAME}MSP": {
			"mspid": "${ORG_NAME}MSP",
			"adminPrivateKey": {
				"path": "/etc/hyperledger/fabric/msp/keystore/key.pem"
			},
			"peers": ["hlf-peer--${ORG_NAME}--peer0"],
			"signedCert": {
				"path": "/etc/hyperledger/fabric/msp/signcerts/cert.pem"
			}
		}
	},
	"peers": {
		"hlf-peer--${ORG_NAME}--peer0": {
			"tlsCACerts": {
				"path": "/etc/hyperledger/fabric/tls/ca.crt"
			},
			"url": "grpc://hlf-peer--${ORG_NAME}--peer0:7051",
			"grpcOptions": {
				"ssl-target-name-override": "peer0.${ORG_NAME}.example.com"
			}
		}
	}
}

